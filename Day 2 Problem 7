Day 2
Problem 7
198 House Robber

TLE -- Recursion
class Solution {
public:
    int money(vector<int>& nums, int n){
        if(n<0)return 0;
        if(n==0)return nums[0];
        int one = nums[n] + money(nums,n-2);
        int two = nums[n-1] + money(nums,n-3);
        return max(one,two);
    }
    int rob(vector<int>& nums) {
        int sz = nums.size();
        int m = money(nums,sz-1);
        return m;

    }
};
